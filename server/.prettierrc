{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "useTabs": true,
  "tabWidth": 2,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/routes/",
		"/middleware/",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}